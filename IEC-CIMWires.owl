                    <rdf:Description rdf:about="http://alliander.com/esm/IEC-CIM/Wires#SinglePhaseKind.s1"/>
                    <rdf:Description rdf:about="http://alliander.com/esm/IEC-CIM/Wires#SinglePhaseKind.s2"/>
                </owl:oneOf>
